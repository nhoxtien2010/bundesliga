<div class="row">
	<div class="col-md-4 col-md-offset-4">


		<%= form_for(@chitiettrandau) do |f| %>
		  <% if @chitiettrandau.errors.any? %>
		    <div id="error_explanation">
		      <h2><%= pluralize(@chitiettrandau.errors.count, "error") %> prohibited this chitiettrandau from being saved:</h2>
		
		      <ul>
		      <% @chitiettrandau.errors.full_messages.each do |message| %>
		        <li><%= message %></li>
		      <% end %>
		      </ul>
		    </div>
		  <% end %>
		
		  <div class="field">
		   <%= f.label :trandau_id, "Match" %><br>    
			    <%= f.select(:trandau_id) do %>
			  		<% Trandau.all.each do |c| -%>
			    	<%= content_tag(:option, Doibong.find(c.doinha_id).ten.to_s+'-'+Doibong.find(c.doikhach_id).ten.to_s+'-'+Vongdau.find(c.vongdau).stt.to_s, value: c.id) %>
			  	<% end %>
				<% end %>
		  </div>
		  <div class="actions">
		    <%= f.submit "Submit", :class=>"btn btn-success" %>
		  </div>
		<% end %>
	</div>
</div>
